<h1>New Task</h1>

<%= form_for(@task, @task_list) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :notes %>
  <%= f.text_area :notes %>
  <%= f.label :start_date %>
  <%= f.date_field :start %>
  <%= f.label :end_date %>
  <%= f.date_field :due %>
  <%= f.submit "Create Task" %>
<% end %>

